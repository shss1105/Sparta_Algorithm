using System;

public class Solution 
{
    public int solution(int[] a, int[] b) 
    {
        int answer = 1234567890;
        int length = a.Length;
        int sum = 0;

        for (int i = 0; i < length; i++)
        {
            sum += a[i] * b[i];
        }

        answer = sum;
        return answer;
    }
}
